{
  "name": "persistent-c",
  "version": "0.1.3",
  "main": "./lib/index.js",
  "description": "A C language step-by-step evaluator written in JavaScript and using persistent (immutable) data structures.",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "node_modules/.bin/babel-tape-runner ./spec/**/*.spec.js | node_modules/.bin/tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epixode/persistent-c.git"
  },
  "keywords": [
    "C",
    "evaluator",
    "persistent"
  ],
  "author": "epixode (https://epixode.fr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/epixode/persistent-c/issues"
  },
  "homepage": "https://github.com/epixode/persistent-c#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "jspm": {
    "main": "./index.js",
    "directories": {
      "lib": "lib"
    },
    "dependencies": {
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }

  "bugs": {
    "url": "https://github.com/epixode/persistent-c/issues"
  },
  "homepage": "https://github.com/epixode/persistent-c#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
